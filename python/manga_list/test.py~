#!/usr/bin/python
# -*- coding: utf-8 -*-

import init_class

def main():
    manga = list()
    manga[0] = init_class.Manga()
    manga[0].nom = "One Piece"
    manga[0].genre = "Action / Aventure / Humour"
    manga[0].nbr = 81
    manga[0].en_cours = True
    init_class.Manga.print_manga(manga[0])

if __name__ == "__main__":
    main()
